// 3rd January 2025
#include <bits/stdc++.h>
using namespace std;

int main()
{
    int c;
    cin >> c;

    while (c--)
    {
        int x;
        cin >> x;

        if (x > 100)
            cout << x - 10 << endl;
        else
            cout << x << endl;
    }
}
