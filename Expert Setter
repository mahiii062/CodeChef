// 3rd January 2025
#include <bits/stdc++.h>
using namespace std;

int main()
{
    int c;
    cin >> c;

    while (c--)
    {
        int x, y;
        cin >> x >> y;

        if (y * 2 >= x)
            cout << "YES" << endl;
        else
            cout << "NO" << endl;
    }
}
