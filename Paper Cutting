// 3rd January 2025
#include <bits/stdc++.h>
using namespace std;

int main()
{
    int c;
    cin >> c;

    while (c--)
    {
        int n, k;
        cin >> n >> k;

        int cut = (n / k) * (n / k);

        cout << cut << endl;
    }
}
